extends layout

block content
    div(class='container')
        div(class='row')
            div(class='col-md-12' class='space-contain-bottom')
    div(class='container listingContainer' style='border-right:1px solid #E7E7E7;')
        div(class='row' style='border-top:1px solid #E7E7E7;border-bottom:1px solid #E7E7E7;')
            div(class='col-md-5 leftAdjust')
                div(id='building-photos' class='carousel slide' data-ride='carousel')
                    ol(class='carousel-indicators')
                        for item, i in suites.imageSrc
                            if i === 0
                                li(data-target='#building-photos' data-slide-to='#{i}' class='active')
                            else
                                li(data-target='#building-photos' data-slide-to='#{i}')
                    div(class='carousel-inner' role='listbox')
                        for item, i in suites.imageSrc
                            if i === 0
                                | 
                                div(class='item active')
                                    div(style='background:url("https://s3-us-west-1.amazonaws.com/creativespacesla/public/#{item}") center center; background-size: cover' class='slider-size')
                                    //- img(src='https://s3-us-west-1.amazonaws.com/creativespacesla/public/#{item}' class='carouselPhoto' style='')
                            else
                                div(class='item')
                                    //- img(src='https://s3-us-west-1.amazonaws.com/creativespacesla/public/#{item}' class='carouselPhoto' style='')
                                    div(style='background:url("https://s3-us-west-1.amazonaws.com/creativespacesla/public/#{item}") center center; background-size: cover' class='slider-size')
                    a(class='carousel-control left' href='#building-photos' role='button' data-slide='prev')
                        span(class='glyphicon glyphicon-chevron-left' aria-hidden='true')
                    a(class='carousel-control right' href='#building-photos' role='button' data-slide='next')
                        span(class='glyphicon glyphicon-chevron-right' aria-hidden='true')
                for item, i in suites.imageSrc
                    if i === 3 || i === 7
                        img(src='https://s3-us-west-1.amazonaws.com/creativespacesla/public/#{item}' data-carousel-number='#{i}' class='thumb mobileHide')
                    else
                        img(src='https://s3-us-west-1.amazonaws.com/creativespacesla/public/#{item}' data-carousel-number='#{i}' class='thumb thumbsRight mobileHide')
            div(class='col-md-5 mobile-height-adjust' style='border-right:1px solid #E7E7E7;')
                h2= suites.address
                h5 #{suites.city}, CA
                p #{suites.description}
            div(class='col-md-3 mobileOnly')
                h4(style='margin-top:30px;') Available Space:
                ul 
                    for val in suites.availableSuites
                        li= val
                br 
                h4 Amenities:
                ul
                    for val in suites.amenities
                        li= val 
                            i(class='fa fa-check' style='float:right;margin-right:56%;color:#9BCA4F')
                br
                br
                i(class='fa fa-building-o' style='margin-right:10px;')
                a(href='http://x.lnimg.com/attachments/B50A5751-E6F9-435B-BBAA-6185CB05EF8A.pdf' style='') Download Building Flyer
            div(class='col-md-2')
                form(role='form' id='emailForm' action='#')
                    div(class='form-group')
                        input(name='name' class='form-control' placeholder='name' style='margin-top:30px; margin-bottom:10px;')
                        input(name='email' type='email' class='form-control' placeholder='email' style='margin-bottom:15px;')
                        input(name='address' type='hidden' value='#{suites.address}')    
                        button(type='submit' class='btn btn-default mobile-button-adjust' style='color:white;background-color:#00B7FF;') Learn More
    div(class='container')
        div(class='row' style='border-right:1px solid #E7E7E7;border-bottom:1px solid #E7E7E7;margin-bottom:9%;')
            div(class='col-md-5 leftAdjust')
                div(id='map-canvas' class='map-canvas map-height' style='width:100%;')
            div(class='col-md-3 mobileHide')
                h4(style='margin-top:30px;') Available Space:
                ul 
                    for val in suites.availableSuites
                        li= val
                br 
                h4 Amenities:
                ul
                    for val in suites.amenities
                        li= val 
                            i(class='fa fa-check' style='float:right;margin-right:47%;color:#9BCA4F')
                br
                br
                i(class='fa fa-building-o' style='margin-right:10px;')
                a(href='http://x.lnimg.com/attachments/B50A5751-E6F9-435B-BBAA-6185CB05EF8A.pdf' target='_blank' style='') Download Building Flyer
                   
    



